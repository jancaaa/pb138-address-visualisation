<?xml version="1.0" encoding="UTF-8"?>
<mapView>
	<!-- all arguments given in this example are required! (optionals are in comments) -->

	<point x="1234" y="5678" title="Tady je nějaký zajímavý místo" />

	<line title="hranice mezi... něčím"> <!-- color<html color>, opacity<0, 1> -->
		<point x="897" y="888" />
		<point x="222" y="458" />
		<point x="3333" y="4444" />
	</line>

	<polygon title="Why not?"> <!-- borderColor<html color>, borderOpacity<0, 1>, borderWidth, fillColor<html color>, fillOpacity<0, 1> -->
		<point x="897" y="888" />
		<point x="222" y="458" />
		<point x="897" y="888" />
		<point x="222" y="458" />
		<point x="897" y="888" />
		<point x="222" y="458" />
	</polygon>

	<circle title="nevím" x="876" y="543" radius="100" /> <!-- borderColor<html color>, borderOpacity<0, 1>, borderWidth, fillColor<html color>, fillOpacity<0, 1> -->
</mapView>
